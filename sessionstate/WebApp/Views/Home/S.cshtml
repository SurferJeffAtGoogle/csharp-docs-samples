@{
    ViewBag.Title = "Session Keys";
}
@model WebApp.Models.SessionVariable

<div class="container body-content">
<div class="row">
<div class="col-md-9">

<p></p>

<form method="post" id="sessionvar-form" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Key"></label>
        <input asp-for="Key" class="form-control" />
        <span asp-validation-for="Key" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="Value"></label>
        <input asp-for="Value" class="form-control" />
        <span asp-validation-for="Value" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-success">Save</button>
</form>

<h2>Session Vars</h2>

<ul>
    @foreach (var key in ViewBag.Keys)
    {
        <li><a href="/Home/S/@key">@key</a></li>
    }
</ul>

</div>
</div>
</div>